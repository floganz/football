<h1>Teams#index</h1>
<p>Find me in app/views/teams/index.html.erb</p>

<%= label :team, "Find team" %>
<%= form_tag teams_search_url, method: 'get', :class => 'form-inline' do %>
	<%= content_tag :div, :class => 'form-group' do %>
		<%= text_field :team, :keyword, :placeholder => 'Type here keyword', :value => '' %>
	<% end %>
	<%= submit_tag "Find team" %>
<% end %>

<%= render 'teams/display' %>
